<div class="row">
	<div class="col-md-6"> 
		<table>
			<tr>
				<th>WatIAM ID</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Program</th>
				<th>Term</th>
				<th>Email</th>

			</tr>
			<% @users.each do |user| %>
				<tr class="user_row" id="user<%= user.username %>">
					<td><%= user.username %></td>
					<td><%= user.firstname %></td>
					<td><%= user.lastname %></td>
					<td><%= user.major %></td>
					<td><%= user.year %></td>
					<td><%= user.email %></td>
				</tr>		
			<% end %>
		</table>
	</div>
	<div class="col-mid-6">	
		
	</div>
</div>